<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.2</TargetFramework>
    <RootNamespace>Wookashi.ExtraText</RootNamespace>
    <AssemblyName>Wookashi.ExtraText</AssemblyName>
    <Version>1.1.0</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Description>Simple package to replace diacritical marks in text.
        Supported languages: Polish, German, French and Czech.</Description>
    <Copyright>Copyright ©2019 Wookashi</Copyright>
    <NeutralLanguage>en</NeutralLanguage>
    <Authors>Wookashi</Authors>
    <PackageId>Wookashi.ExtraText</PackageId>
    <PackageReleaseNotes>New languages added (French, Czech)</PackageReleaseNotes>
    <PackageTags>diacritical replace chars polish german french czech</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Wookashi/Wookashi.ExtraText.git</RepositoryUrl>
    <Owners>Wookashi</Owners>
  </PropertyGroup>
  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="." />
    <None Include="..\README.md" Pack="true" PackagePath="."/>
  </ItemGroup>
</Project>
